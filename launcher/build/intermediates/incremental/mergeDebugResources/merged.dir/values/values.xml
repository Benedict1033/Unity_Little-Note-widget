<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="light_blue_200">#FF81D4FA</color>
    <color name="light_blue_50">#FFE1F5FE</color>
    <color name="light_blue_600">#FF039BE5</color>
    <color name="light_blue_900">#FF01579B</color>
    <dimen name="widget_margin">0dp</dimen>
    <item name="unitySurfaceView" type="id"/>
    <string name="add_widget">Add widget</string>
    <string name="app_name">Little Note 2021-12-10</string>
    <string name="app_widget_description">This is an app widget description</string>
    <string name="appwidget_text">EXAMPLE</string>
    <string name="game_view_content_description">Game view</string>
    <style name="BaseUnityTheme" parent="android:Theme.Holo.Light.NoActionBar.Fullscreen"/>
    <style name="Theme.NewFolder.AppWidgetContainer" parent="Theme.NewFolder.AppWidgetContainerParent"/>
    <style name="Theme.NewFolder.AppWidgetContainerParent" parent="@android:style/Theme.DeviceDefault">
        <item name="appWidgetRadius">8dp</item>
        <item name="appWidgetPadding">4dp</item>
        <item name="appWidgetInnerRadius">4dp</item>
    </style>
    <style name="UnityThemeSelector" parent="BaseUnityTheme">
        <item name="android:windowBackground">@android:color/black</item>
    </style>
    <style name="UnityThemeSelector.Translucent" parent="@style/UnityThemeSelector">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="Widget.NewFolder.AppWidget.Container" parent="android:Widget">
        <item name="android:id">@android:id/background</item>
        <item name="android:background">?android:attr/colorBackground</item>
    </style>
    <style name="Widget.NewFolder.AppWidget.InnerView" parent="android:Widget">
        <item name="android:background">?android:attr/colorBackground</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
    </style>
    <declare-styleable name="AppWidgetAttrs">
        <attr format="dimension" name="appWidgetPadding"/>
        <attr format="dimension" name="appWidgetInnerRadius"/>
        <attr format="dimension" name="appWidgetRadius"/>
    </declare-styleable>
</resources>